<?xml version="1.0" encoding="utf-8" ?>
<!-- TODO: Might be a better idea with tighter integration to model by using attributes on each
     tag that corresponds to an actual model element. 
     E.g. <PlayfieldElement NumTreasures="2" SquareType="AmmoStorage" />-->
<LabyrinthLevel>
  <Playfield width="5" height="5">
    <Row>
      <Empty />
      <Empty treasures="1"/>
      <FitnessStudio treasures="2"/>
      <FitnessStudio />
      <Teleporter teleporterIndex="3"/>
    </Row>
    <Row>
      <AmmoStorage />
      <Empty />
      <Empty />
      <Empty />
      <Teleporter teleporterIndex="5"/>
    </Row>
    <Row>
      <Empty />
      <Empty />
      <Empty />
      <CementStorage />
      <HamsterStorage />
    </Row>
    <Row>
      <Empty />
      <Teleporter teleporterIndex="1"/>
      <Empty />
      <Empty />
      <Teleporter teleporterIndex="2"/>
    </Row>
    <Row>
      <Empty />
      <Empty />
      <Empty />
      <Teleporter teleporterIndex="4"/>
      <Empty />
    </Row>
  </Playfield>

  <LabyrinthExits>
    <NormalExit mazeEdge="left" y="3"/>
    <HiddenExit mazeEdge="right" y="0"/>
    <HiddenExit mazeEdge="bottom" x="1"/>
    <NormalExit mazeEdge="top" x="4"/>
  </LabyrinthExits>
    
  <HorizontalWalls>
    <HorizontalWallSegment>
      <HasHamster />
      <w_x>1</w_x>
      <y>0</y>
    </HorizontalWallSegment>

  </HorizontalWalls>

  <VerticalWalls>
    <VerticalWallSegment>
      <w_y>1</w_y>
      <x>0</x>
    </VerticalWallSegment>
  </VerticalWalls>

  <StartingPositions>
    <PlayerPosition x="3" y="2" />
    <PlayerPosition x="4" y="4" />
  </StartingPositions>
  
  <Centaur>
    <CentaurPosition x="1" y="0" />
    <CentaurPath>
      
    </CentaurPath>
  </Centaur>
  
</LabyrinthLevel>